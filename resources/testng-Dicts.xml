<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Dictionary Suite" parallel="none">
  <test name="NCI - Testing Dictionaries" preserve-order="true">
  <!--
  The browser parameter is set in pom.xml as part of invoking
  the maven-surefire-plugin.
  -->

  <!--
    <parameter name="browser" value="Chrome" />
  -->
   <classes>
    <class name="gov.nci.dictionary.DictDrugs_Test" />
    <class name="gov.nci.dictionary.DictGenetics_Test" />
    <class name="gov.nci.dictionary.DictTerms_Test" />
    <class name="gov.nci.dictionary.DictTermsES_Test">
      <methods>
        <include name="TitleVisible" />
      </methods>
    </class>

    <class name="gov.nci.dictionary.NewDrugs_Test">
      <methods>
        <include name="HeaderVisible"/>
        <include name="HeaderDisplay"/>
        <include name="TitleVisible"/>
        <include name="TitleDisplay"/>
        <include name="RadioVisible"/>
        <include name="RadioStartsWithSelected"/>
        <include name="SearchInputVisible"/>
        <include name="SearchBtnVisible"/>
        <include name="AZListVisible"/>
        <include name="AZListHome"/>
        <include name="AZListHomeUrl"/>
        <include name="AZListSelectLetter"/>
        <include name="AZListSelectLetterTestUrl"/>
        <include name="SubmitSearchStartsWith"/>
        <include name="SubmitSearchStartsWithUrl"/>
      </methods>
    </class>
    <!--
        <class name="gov.nci.dictionary.NewGenetics_Test" />
        <class name="gov.nci.dictionary.NewTerms_Test" />
        <class name="gov.nci.dictionary.NewTermsES_Test" />
      -->

   </classes>
  </test>
</suite>

